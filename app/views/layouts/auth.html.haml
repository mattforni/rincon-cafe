<!DOCTYPE html>
%html
  %head
    %title='Rincon Cafe'
    = stylesheet_link_tag 'application', media: 'all', 'data-turbolinks-track' => true
    = stylesheet_link_tag 'auth', media: 'all', 'data-turbolinks-track' => true
    = javascript_include_tag 'application', 'data-turbolinks-track' => true
    = csrf_meta_tags
  %body
    = render partial: 'application/toast', locals: {toast: toast_message, error: true}

    #content
      = yield

    = render partial: 'application/copyright'

